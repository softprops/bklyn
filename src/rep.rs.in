#[derive(Debug, Deserialize)]
pub struct Value {
    pub timestamp: String,
    pub value: u64
}

#[derive(Debug, Deserialize)]
pub struct Metrics {
    pub metrics: Vec<Value>
}

#[derive(Debug, Deserialize)]
pub struct Summary {
    pub name: String,
    #[serde(rename="cpuUsage")]
    pub cpu_usage: u64,
    #[serde(rename="memUsage")]
    pub mem_usage: u64
}

#[derive(Debug, Deserialize)]
pub struct Aggregate {
    pub average: u64,
    pub percentile: u64,
    pub max: u64
}

#[derive(Debug, Deserialize)]
pub struct Aggregates {
    pub minute: Aggregate,
    pub hour: Aggregate,
    pub day: Aggregate
}

#[derive(Debug, Deserialize)]
pub struct Stats {
    pub uptime: u64,
    pub stats: BTreeMap<String, Aggregates>
}
